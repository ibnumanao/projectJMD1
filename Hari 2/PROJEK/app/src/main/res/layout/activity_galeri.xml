<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".galeri">
    <TextView
        android:id="@+id/judul"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MY GALERY"
        android:textSize="30sp"
        android:textAlignment="center"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:fontFamily="monospace" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="139dp"
        android:layout_height="294dp"
        app:srcCompat="@drawable/catsoldiera"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="298dp"
        android:layout_below="@id/judul"
        android:layout_alignStart="@id/judul"
        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="139dp"
        android:layout_height="294dp"
        app:srcCompat="@drawable/catsoldierad"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="-10dp"
        android:layout_below="@id/judul"
        android:layout_alignEnd="@id/judul"
        android:paddingTop="-20sp"
        />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="139dp"
        android:layout_height="294dp"
        app:srcCompat="@drawable/catsoldierad"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="-10dp"
        android:layout_below="@id/imageView1"
        android:layout_alignStart="@id/imageView1"
        />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="139dp"
        android:layout_height="294dp"
        app:srcCompat="@drawable/catsoldierac"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="138dp"
        android:layout_below="@id/imageView2"
        android:layout_alignEnd="@id/imageView2"
        />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="139dp"
        android:layout_height="294dp"
        app:srcCompat="@drawable/catsoldierab"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="298dp"
        android:layout_below="@id/imageView3"
        android:layout_alignStart="@id/imageView3"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="139dp"
        android:layout_height="294dp"
        app:srcCompat="@drawable/catsoldier"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="138dp"
        android:layout_below="@id/imageView4"
        android:layout_alignEnd="@id/imageView4" />


</RelativeLayout>